<%- include('header'); -%>


    <div class="box" id="heading">
        <h1>
            Trash
        </h1>
    </div>

    <div class="box">

        <% newListItems.forEach((item)=>{ %>
            <form action="/delete" method="post">
                <div class="item">
                    <input class="checkbox" type="checkbox" name="checkbox" value="<%=item._id%>" onChange="this.form.submit()">
                    <p>
                        <%= item.name %>
                    </p>
                </div>
            </form>
            <%})%>

    </div>


    <%- include('footer'); -%>
